<div class="row mb-3">
    <label for="inputEmail" class="col-sm-2 col-form-label">ModelNumber</label>
    <div class="col-sm-10">
        <input type="text" class="form-control" id="ModelNumber">
    </div>
</div>

<div class="row mb-3">
    <label for="inputEmail" class="col-sm-2 col-form-label">Brand</label>
    <div class="col-sm-10">
        <input type="text" class="form-control" id="Brand">
    </div>
</div>

<div class="row mb-3">
    <label for="inputEmail" class="col-sm-2 col-form-label">Capacity</label>
    <div class="col-sm-10">
        <select class="form-control form-select form-control-a" id="Capacity">
            <option>Select Capacity</option>
            <option value="1">1 Ton</option>
            <option value="1.1">1.1 Ton</option>
            <option value="1.3">1.3 Ton</option>
            <option value="1.5">1.5 Ton</option>
            <option value="1.6">1.6 Ton</option>
            <option value="1.8">1.8 Ton</option>
            <option value="1.95">1.95 Ton</option>
            <option value="2.2">2.2 Ton</option>
            <option value="2.5">2.5 Ton</option>
            <option value="3">3 Ton</option>
        </select>
    </div>
</div>

<div class="row mb-3">
    <label for="inputEmail" class="col-sm-2 col-form-label">Refrigerant</label>
    <div class="col-sm-10">
        <select class="form-control form-select form-control-a" id="Refrigerant">
            <option>Select Refrigerant</option>
            <option value="R32">R32</option>
            <option value="R410">R410</option>
        </select>
    </div>
</div>

<div class="row mb-3">
    <label for="inputEmail" class="col-sm-2 col-form-label">Voltage</label>
    <div class="col-sm-10">
        <select class="form-control form-select form-control-a" id="Voltage">
            <option>Select Voltage</option>
            <option value="220">220</option>
            <option value="etc">etc</option>
        </select>
    </div>
</div>

<div class="row mb-3">
    <label for="inputEmail" class="col-sm-2 col-form-label">Color</label>
    <div class="col-sm-10">
        <select class="form-control form-select form-control-a" id="Color">
            <option>Select Color</option>
            <option value="White">White</option>
            <option value="Black">Black</option>
            <option value="Grey">Grey</option>
            <option value="Silver">Silver</option>
        </select>
    </div>
</div>

<div class="row mb-3">
    <label for="inputEmail" class="col-sm-2 col-form-label">Warranty</label>
    <div class="col-sm-10">
        <select class="form-control form-select form-control-a" id="Warranty">
            <option>Select Warranty</option>
            <option value="1">1 Year</option>
            <option value="2">2 Year</option>
            <option value="3">3 Year</option>
            <option value="4">4 Year</option>
        </select>
    </div>
</div>

<div class="row mb-3">
    <label for="inputEmail" class="col-sm-2 col-form-label">StarRating</label>
    <div class="col-sm-10">
        <select class="form-control form-select form-control-a" id="StarRating">
            <option>Select Star Rating</option>
            <option value="1">1 Star</option>
            <option value="2">2 Star</option>
            <option value="3">3 Star</option>
            <option value="4">4 Star</option>
            <option value="5">5 Star</option>
        </select>
    </div>
</div>

<div class="row mb-3">
    <label for="inputEmail" class="col-sm-2 col-form-label">BrochureUrl</label>
    <div class="col-sm-10">
        <input type="file" class="form-control" id="BUrl">
